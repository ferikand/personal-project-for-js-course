{
  "version": 2,
  "rewrites": [
    {
      "source": "/add-product",
      "destination": "/js/server.js"
    },
    {
      "source": "/delete-product/:id",
      "destination": "/js/server.js"
    },
    {
      "source": "/update-product/:id",
      "destination": "/js/server.js"
    },
    {
      "source": "/products",
      "destination": "/js/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/js/server.js"
    }
  ]
}
